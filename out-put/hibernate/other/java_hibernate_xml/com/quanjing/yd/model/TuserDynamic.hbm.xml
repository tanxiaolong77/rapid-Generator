<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.quanjing.yd.model">
    <class name="TuserDynamic" table="t_user_dynamic">

        <id name="id" column="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
		
		
		
		<property 
			name="userId" 
			column="user_id" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="true" 
			unique="false" 
			length="19"/>
		
		<property 
			name="act" 
			column="act" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="dataType" 
			column="data_type" 
			update="true"
            insert="true"
            type="java.lang.Integer"
			not-null="false" 
			unique="false" 
			length="10"/>
		
		<property 
			name="content" 
			column="content" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="3000"/>
		
		<property 
			name="createTime" 
			column="create_time" 
			update="true"
            insert="true"
            type="java.util.Date"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="address" 
			column="address" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="100"/>
		
		<property 
			name="longitude" 
			column="longitude" 
			update="true"
            insert="true"
            type="java.lang.Float"
			not-null="false" 
			unique="false" 
			length="12"/>
		
		<property 
			name="latitude" 
			column="latitude" 
			update="true"
            insert="true"
            type="java.lang.Float"
			not-null="false" 
			unique="false" 
			length="12"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

