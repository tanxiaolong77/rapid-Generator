<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.quanjing.yd.model">
    <class name="Test" table="test">

        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
		
		
		
		<property 
			name="name" 
			column="name" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="true" 
			unique="false" 
			length="128"/>
		
		<property 
			name="pnt" 
			column="pnt" 
			update="true"
            insert="true"
            type="java.lang.Object"
			not-null="true" 
			unique="false" 
			length="0"/>
		
		<property 
			name="line" 
			column="line" 
			update="true"
            insert="true"
            type="java.lang.Object"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<property 
			name="pgn" 
			column="pgn" 
			update="true"
            insert="true"
            type="java.lang.Object"
			not-null="false" 
			unique="false" 
			length="0"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

