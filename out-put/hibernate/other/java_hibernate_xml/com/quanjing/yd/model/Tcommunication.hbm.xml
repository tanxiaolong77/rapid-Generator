<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.quanjing.yd.model">
    <class name="Tcommunication" table="t_communication">

        <id name="id" column="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
		
		
		
		<property 
			name="userId" 
			column="user_id" 
			update="true"
            insert="true"
            type="java.lang.Long"
			not-null="true" 
			unique="false" 
			length="19"/>
		
		<property 
			name="communicationPhone" 
			column="communication_phone" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="20"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

    </class>
</hibernate-mapping>

